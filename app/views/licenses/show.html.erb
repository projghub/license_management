<%- model_class = License -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:purchase_order_id) %>:</strong></dt>
  <dd><%= @license.purchase_order_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:purchase_order_item_id) %>:</strong></dt>
  <dd><%= @license.purchase_order_item_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:purchase_order_item_type_id) %>:</strong></dt>
  <dd><%= @license.purchase_order_item_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:account_id) %>:</strong></dt>
  <dd><%= @license.account_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quantity) %>:</strong></dt>
  <dd><%= @license.quantity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:notes) %>:</strong></dt>
  <dd><%= @license.notes %></dd>
  <dt><strong><%= model_class.human_attribute_name(:start_date) %>:</strong></dt>
  <dd><%= @license.start_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:end_date) %>:</strong></dt>
  <dd><%= @license.end_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:exuberance_coordinator_id) %>:</strong></dt>
  <dd><%= @license.exuberance_coordinator_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:funding_source) %>:</strong></dt>
  <dd><%= @license.funding_source %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status_id) %>:</strong></dt>
  <dd><%= @license.status_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:deleted) %>:</strong></dt>
  <dd><%= @license.deleted %></dd>
  <dt><strong><%= model_class.human_attribute_name(:created_by) %>:</strong></dt>
  <dd><%= @license.created_by %></dd>
  <dt><strong><%= model_class.human_attribute_name(:modified_by) %>:</strong></dt>
  <dd><%= @license.modified_by %></dd>
  <dt><strong><%= model_class.human_attribute_name(:net_suite_account_id) %>:</strong></dt>
  <dd><%= @license.net_suite_account_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              licenses_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_license_path(@license), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              license_path(@license),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
